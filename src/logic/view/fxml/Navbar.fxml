<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="main" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="622.0" prefWidth="180.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="myRoomsBtn" mnemonicParsing="false" onMouseClicked="#goToRooms" prefWidth="150.0" text="My Rooms" />
            <Button fx:id="myResBtn" mnemonicParsing="false" onMouseClicked="#goToReservations" prefWidth="150.0" text="My Reservations" />
            <Button fx:id="myGroupsBtn" mnemonicParsing="false" onMouseClicked="#goToGroups" prefWidth="150.0" text="My Groups" />
            <Button fx:id="myReviewsBtn" mnemonicParsing="false" onMouseClicked="#goToReviews" prefWidth="150.0" text="My Reviews" />
            <Button fx:id="postRoomBtn" mnemonicParsing="false" onMouseClicked="#goToPostRoom" prefWidth="150.0" text="Post Room" />
            <Button fx:id="searchRoomBtn" mnemonicParsing="false" onMouseClicked="#goToSearch" prefWidth="150.0" text="Search Rooms" />
            <Button fx:id="logOutBtn" mnemonicParsing="false" onMouseClicked="#doLogOut" text="Log Out">
               <VBox.margin>
                  <Insets top="250.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </VBox>
   </left>
</BorderPane>
